# {{ ansible_managed }}

[Unit]
Description=A Quarkus service named {{ quarkus_app_service_name }}
After=syslog.target network.target

[Service]
EnvironmentFile=-{{ quarkus_deploy_systemd_service_env_conf }}
User={{ quarkus_app_user }}
Group={{ quarkus_app_group }}
#
ExecStart={{ quarkus_java_home }}/bin/java -jar {{ quarkus_path_to_folder_to_deploy }}/quarkus-run.jar

[Install]
WantedBy=multi-user.target
